{
  "eslint.autoFixOnSave": true,
  "eslint.run": "onSave",
  "suggest.noselect": false,
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "prettier.trailingComma": "none",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "json",
    "python"
  ],
  "pyright.enable": true,
  "python.jediEnabled": false,
  "rest-client.showHeaders": false
}
